<div class="container">
  <h1 class="white">All stories</h1>
  <% @stories.each do |story| %>
    <div class="col-md-9">
      <h3 class="white"><%= link_to story.title, business_story_path(@business, story) %></h3>
    </div>
    <div class="col-md-3">
      <% story.photos.each do |photo| %>
        <%= cl_image_tag photo.key %>
        <% end %>
    </div>
    <% end %>
</div>
